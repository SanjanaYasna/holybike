<div class="record-row <%= cycle "odd", "even", name: "row-cycler" %> flexbox horizontal justify-space-between">
  <div class="flexbox vertical stretch">
        <%= link_to station_path(station.identifier) do %>
          <%= station.identifier %>: <%= station.name %> (<%= station.address %>)
        <% end %>
        <%= "#{station.docked_bikes.count} bikes"%>
    </div>
</div>


